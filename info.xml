<?xml version="1.0"?>
<extension key="de.systopia.anonymiser" type="module">
  <file>anonymiser</file>
  <name>Contact Anonymiser</name>
  <description>Enables you to anonymise contacts without losing the statistical information</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.anonymiser</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.anonymiser</url>
    <url desc="Support">https://github.com/systopia/de.systopia.anonymiser/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>1.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
    <ver>5.28</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Anonymiser</namespace>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
